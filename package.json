    "html-webpack-plugin": "2.29.0",
    "html2canvas": "^1.0.0-alpha.12",
    "jest": "20.0.4",
    "less": "^3.0.4",
    "less-loader": "^4.1.0",
    "md5": "^2.2.1",
    "nprogress": "^0.2.0",
    "object-assign": "4.1.1",
    "pako": "^1.0.6",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.8",
    "promise": "8.0.1",
    "prop-types": "^15.6.1",
    "qrcode": "^1.2.2",
    "qrcode.react": "^0.8.0",
    "qs": "^6.5.2",
    "raf": "3.4.0",
    "react": "^16.4.1",
    "react-copy-to-clipboard": "^5.0.1",
